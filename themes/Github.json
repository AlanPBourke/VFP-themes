	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6a737d"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#032f62"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language.null",
				"variable.language.this",
				"support.type.primitive",
				"support.function",
				"support.variable.dom",
				"support.variable.property",
				"support.variable.property",
				"meta.property-name",
				"meta.property-value",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#d73a49"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"meta.function-call",
				"entity.other.attribute-name",
				"entity.name.function.shell"
			],
			"settings": {
				"foreground": "#6f42c1"
			}
		},
		{
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#22863a"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}